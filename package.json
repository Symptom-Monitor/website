  },
  "dependencies": {
    "next": "10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "eslint": "^7.13.0",
    "eslint-config-xo": "^0.33.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "typescript": "^4.0.5"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "xo-react"
    ]
  },
  "husky": {
